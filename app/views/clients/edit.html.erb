<h1>Edit Client</h1>
<div class='row'>
  <div class='col-md-6 col-md-offset-3'>
    <%= form_for(@client) do |f| %>
      <%= render 'shared/error_messages' %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :titleXIX %>
      <%= f.text_field :titleXIX, class: 'form-control' %>

      <%= f.submit 'Update Client', class: 'btn btn-sm btn-danger' %>
    <% end %>
  </div>
</div>
